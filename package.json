{
  "name": "njyag",
  "version": "1.0.0",
  "description": "New Jersey Youth and Government Bill Submission System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend": "cd frontend && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend\"",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "create-admin": "node scripts/createAdmin.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^2.0.22"
  }
}
